{
  "name": "Code: F1",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Verhindert das ablaufen der Sessions bei der Fahrplanauskunft der Deutschen Bahn",
  "homepage_url": "http://blattspinat.com",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "default_locale": "en",
  "permissions": [
    "https://reiseauskunft.bahn.de/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://reiseauskunft.bahn.de/*"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}